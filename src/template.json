                "syntax": {
                    "attribute": {
                        "color": "$subtle"
                    },
                    "boolean": {
                        "color": "$rose"
                    },
                    "comment": {
                        "color": "$muted",
                        "font_style": "italic"
                    },
                    "comment.doc": {
                        "color": "$subtle"
