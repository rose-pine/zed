{
    "$schema": "https://zed.dev/schema/themes/v0.2.0.json",
    "name": "$name",
    "author": "Kainoa Kanter <kainoa@t1c.dev>",
    "themes": [
        {
            "appearance": "$type",
            "name": "$name",
            "style": {
                "editor.foreground": "$text",
                "editor.background": "$base",
                "editor.gutter.background": "$base",
                "editor.active_line.background": "$surface",
                "editor.line_number": "$muted",
                "editor.active_line_number": "$text",
                "editor.indent_guide": "$muted88",
                "editor.indent_guide_active": "$subtleaa",
                "editor.invisible": "$muted",
                "editor.wrap_guide": "$muted44",
                "editor.active_wrap_guide": "$muted88",
                "editor.document_highlight.read_background": "$foam1a",
                "editor.document_highlight.write_background": "$muted66",
                "editor.subheader.background": "$surface",
