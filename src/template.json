                    {
                        "cursor": "$text",
                        "background": "$text",
                        "selection": "$text22"
                    },
                    {
                        "cursor": "$foam",
                        "background": "$foam",
                        "selection": "$foam44"
                    },
                    {
                        "cursor": "$iris",
                        "background": "$iris",
                        "selection": "$iris44"
                    },
                    {
                        "cursor": "$pine",
                        "background": "$pine",
                        "selection": "$pine44"
                    },
                    {
                        "cursor": "$love",
                        "background": "$love",
                        "selection": "$love44"
                    }
