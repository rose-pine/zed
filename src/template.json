                    "predictive": {
                        "color": "$subtle"
                    },
                    "preproc": {
                        "color": "$gold"
                    },
                    "primary": {
                        "color": "$iris"
                    },
                    "property": {
                        "color": "$text"
                    },
                    "punctuation": {
                        "color": "$subtle"
                    },
                    "punctuation.bracket": {
                        "color": "$subtle"
                    },
                    "punctuation.delimiter": {
                        "color": "$subtle"
                    },
                    "punctuation.list_marker": {
                        "color": "$subtle",
                        "font_weight": 700
                    },
                    "ion.special": {
                        "color": "$pine"
                    },
                    "string": {
                        "color": "$gold"
                    },
                    "string.escape": {
                        "color": "$gold"
                    },
                    "string.regex": {
                        "color": "$gold"
                    },
                    "string.special": {
                        "color": "$pine"
                    },
                    "string.special.symbol": {
                        "color": "$pine"
                    },
                    "tag": {
                        "color": "$iris"
                    },
                    "text.literal": {
                        "color": "$gold"
                    },
                    "title": {
                        "color": "$love",
                        "font_weight": 700
                    },
                    "type": {
                        "color": "$foam"
                    },
                    "variable": {
                        "color": "$text"
                    },
                    "variable.special": {
                        "color": "$foam"
                    },
                    "variant": {
                        "color": "$rose"
                    }
                }
            }
        }
    ]
